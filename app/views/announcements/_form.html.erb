<%= simple_form_for(@announcement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :title %>
    <%= f.input :location %>
    <%= f.association :cohort %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
