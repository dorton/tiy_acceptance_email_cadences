<%= simple_form_for(@code_challenge) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :newline_url %>
    <%= f.input :description %>
    <%= f.input :weeks_out, collection: 1..10 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
