<p id="notice"><%= notice %></p>

<h1>Cohorts</h1>

<ul class="collection with-header">
  <li class="collection-header"><h4><%= current_user.name %> Cohorts</h4></li>
  <% @cohorts.each do |cohort| %>
    <li class="collection-item"><%= link_to cohort.start_date.strftime('%m/%d/%Y'), cohort_path(cohort) %></li>
  <% end %>
</ul>

<br>

<%= link_to 'New Cohort', new_cohort_path, class: "btn" %>
